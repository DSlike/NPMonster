{
  "name": "NPMonster",
  "version": "1.0.0",
  "description": "GUI client for NodeJS packages management",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "gulp watchDebug",
    "build": "gulp productionMode",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=app/imgs/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=app/imgs/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=app/imgs/icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DSlike/NPMonster.git"
  },
  "keywords": [
    "NPM",
    "NodeJS",
    "ElectronApp",
    "Desktop"
  ],
  "author": "Daniel Votintsev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DSlike/NPMonster/issues"
  },
  "homepage": "https://github.com/DSlike/NPMonster#readme",
  "dependencies": {
    "electron-build": "0.0.3",
    "electron-packager": "^9.1.0",
    "electron-prebuilt": "^1.4.13",
    "node-cmd": "^3.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-gzip": "^1.4.0",
    "gulp-pipe": "^1.0.4",
    "gulp-sass": "^3.1.0",
    "gulp-streamify": "^1.0.2",
    "gulp-uglify": "^2.1.2",
    "gulp-watch": "^4.3.11",
    "path": "^0.12.7",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1",
    "vinyl-source-stream": "^1.1.0"
  }
}
